{
  // Vercel Project Configuration
  // Docs: https://vercel.com/docs/project-configuration
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": true,
  "buildCommand": "npm run build",
  "outputDirectory": ".next"
}
